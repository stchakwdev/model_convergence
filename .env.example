# API Keys for Model Access
# Copy this file to .env and add your actual API keys

# OpenRouter API Key (unified access to 300+ models - PRIMARY)
# Get your free key at: https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Legacy API Keys (optional - for direct provider access)
# OpenAI API Key (for direct GPT access)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for direct Claude access) 
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Replicate API Token (for open-source models like Llama)
REPLICATE_API_TOKEN=your_replicate_token_here

# Optional: Weights & Biases for experiment tracking
WANDB_API_KEY=your_wandb_key_here

# Configuration
# Set to 'true' to use cached responses and avoid API costs during development
USE_CACHE=true

# Maximum number of API requests per minute (to respect rate limits)
MAX_REQUESTS_PER_MINUTE=60

# Default model configurations
DEFAULT_TEMPERATURE=0.0
DEFAULT_MAX_TOKENS=500